    <script>
        // Test data and answers
        const correctAnswers = {
            q1: 'b', q2: 'b', q3: 'a', q4: 'c', q5: 'b', q6: 'a', q7: 'b', q8: 'c', q9: 'd', q10: 'b',
            q11: 'b', q12: 'c', q13: 'a', q14: 'c', q15: 'c', q16: 'c', q17: 'c', q18: 'a', q19: 'a', q20: 'c',
            q21: 'b', q22: 'a', q23: 'a', q24: 'c', q25: 'a', q26: 'a', q27: 'b', q28: 'a', q29: 'c', q30: 'b',
            q31: 'c', q32: 'b', q33: 'c', q34: 'd', q35: 'c', q36: 'b', q37: 'b', q38: 'c', q39: 'b', q40: 'c',
            q41: 'b', q42: 'b', q43: 'b', q44: 'b', q45: 'c', q46: 'b', q47: 'a', q48: 'b', q49: 'b', q50: 'c'
        };

        const questionTitles = {
            q1: "Water molecule point group", q2: "Benzene symmetry elements", q3: "Ammonia point group", 
            q4: "Inversion center", q5: "Enantiomer conversion", q6: "Trans-dichloroethene", q7: "D₃ₕ symmetry", 
            q8: "C₃ rotation", q9: "Methane symmetry", q10: "Linear molecule CO₂", q11: "Cis-dichloroethene", 
            q12: "Highest symmetry", q13: "S₄ improper rotation", q14: "MO theory false statement", 
            q15: "Antibonding nodes", q16: "C₂ MO configuration", q17: "MO theory false statement", 
            q18: "SE vs BO difference", q19: "LCAO equation", q20: "Bonding and antibonding", 
            q21: "O₂ vs O₂⁻ bond strength", q22: "Highest energy MO", q23: "N₂⁺ bond order", 
            q24: "Paramagnetic molecule", q25: "π-bonding impossibility", q26: "l=3 magnetic numbers", 
            q27: "Orbital shape determinant", q28: "n=4 possible l values", q29: "Spin quantum values", 
            q30: "5d orbitals count", q31: "Invalid quantum numbers", q32: "4f electron maximum", 
            q33: "l=2 designation", q34: "5f orbital values", q35: "n=5 total orbitals", 
            q36: "Hund's rule violation", q37: "p orbital azimuthal", q38: "Pauli exclusion", 
            q39: "SF₄ geometry", q40: "Bent geometry", q41: "PCl₅ electron geometry", 
            q42: "XeF₄ geometry", q43: "Trigonal planar", q44: "Tetrahedral angle", 
            q45: "Non-influencing factor", q46: "ClF₃ geometry", q47: "SF₆ lone pairs", 
            q48: "Lone pair space", q49: "IF₅ geometry", q50: "Linear geometry"
        };

        // Timer variables
        let timerInterval;
        let timeRemaining = 0;
        let timerRunning = false;
        let studentResponses = JSON.parse(localStorage.getItem('chm202Responses') || '[]');

        // Initialize page
        document.addEventListener('DOMContentLoaded', function() {
            setupEventListeners();
            updateAdminStats();
        });

        function setupEventListeners() {
            // Add click listeners to all options
            document.querySelectorAll('.option').forEach(option => {
                option.addEventListener('click', function() {
                    const radio = this.querySelector('input[type="radio"]');
                    if (radio) {
                        radio.checked = true;
                        // Remove selected class from siblings
                        this.parentNode.querySelectorAll('.option').forEach(opt => {
                            opt.classList.remove('selected');
                        });
                        // Add selected class to clicked option
                        this.classList.add('selected');
                    }
                });
            });
        }

        function submitTest() {
            const studentName = document.getElementById('studentName').value.trim();
            if (!studentName) {
                alert('Please enter your name before submitting the test.');
                return;
            }

            const form = document.getElementById('chemistryTest');
            const formData = new FormData(form);
            const answers = {};
            let answeredCount = 0;

            // Collect answers
            for (let i = 1; i <= 50; i++) {
                const answer = formData.get(`q${i}`);
                if (answer) {
                    answers[`q${i}`] = answer;
                    answeredCount++;
                }
            }

            if (answeredCount < 50) {
                if (!confirm(`You have only answered ${answeredCount} out of 50 questions. Do you want to submit anyway?`)) {
                    return;
                }
            }

            // Calculate score
            let correctCount = 0;
            const detailedResults = [];

            for (let i = 1; i <= 50; i++) {
                const questionKey = `q${i}`;
                const userAnswer = answers[questionKey] || '';
                const correctAnswer = correctAnswers[questionKey];
                const isCorrect = userAnswer === correctAnswer;
                
                if (isCorrect) correctCount++;
                
                detailedResults.push({
                    question: i,
                    title: questionTitles[questionKey],
                    userAnswer,
                    correctAnswer,
                    isCorrect
                });
            }

            const percentage = Math.round((correctCount / 50) * 100);
            
            // Store response
            const response = {
                name: studentName,
                answers,
                score: correctCount,
                percentage,
                timestamp: new Date().toLocaleString(),
                detailedResults
            };
            
            studentResponses.push(response);
            localStorage.setItem('chm202Responses', JSON.stringify(studentResponses));

            // Show results
            displayResults(correctCount, percentage, detailedResults);
            
            // Show thank you message
            document.getElementById('thankYouMessage').classList.add('show');
            
            // Scroll to results
            document.getElementById('results').scrollIntoView({ behavior: 'smooth' });
            
            // Update admin stats
            updateAdminStats();
        }

        function displayResults(score, percentage, detailedResults) {
            const resultsDiv = document.getElementById('results');
            const scoreTitle = document.getElementById('scoreTitle');
            const scoreText = document.getElementById('scoreText');
            const detailedDiv = document.getElementById('detailedResults');

            scoreText.innerHTML = `You scored ${score} out of 50 questions correct (${percentage}%)`;

            // Style based on performance
            resultsDiv.classList.remove('score-good', 'score-average', 'score-poor');
            if (percentage >= 70) {
                resultsDiv.classList.add('score-good');
                scoreTitle.textContent = '🎉 Excellent Performance!';
            } else if (percentage >= 50) {
                resultsDiv.classList.add('score-average');
                scoreTitle.textContent = '👍 Good Effort!';
            } else {
                resultsDiv.classList.add('score-poor');
                scoreTitle.textContent = '📚 Keep Studying!';
            }

            // Show detailed results with explanations
            let detailedHTML = '<h3>Question-by-Question Results:</h3>';
            detailedResults.forEach(result => {
                const statusClass = result.isCorrect ? 'correct' : 'incorrect';
